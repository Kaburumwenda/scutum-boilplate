{
	"name": "scutum_admin_vuejs",
	"version": "2.5.0",
	"description": "Scutum Admin Template (Vue.js version)",
	"author": "tzd",
	"private": true,
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"sc-dev": "nuxt --port 3103",
		"sc-install": "yarn install --ignore-engines",
		"sc-install:npm": "npm run install",
		"sc-setup": "yarn sc-install && yarn sc-uikit",
		"sc-setup:npm": "npm run sc-install && npm run sc-uikit",
		"sc-update": "yarn sc-cleanup && ncu -u",
		"sc-update:npm": "npm run sc-cleanup && ncu -u -i",
		"sc-cleanup": "rm -rf node_modules && rm -rf yarn.lock",
		"sc-uikit": "node ./build/custom-uikit.js",
		"sc-mdi": "node ./build/copy-mdi.js"
	},
